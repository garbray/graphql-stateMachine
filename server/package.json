{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {},
  "scripts": {
    "test": "jest --runInBand"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnviroment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "glogal": {
        "statements": 50,
        "branches": 90,
        "functions": 0,
        "lines": 0
      }
    },
    "resetMocks": true
  }
}
